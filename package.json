{
    "name": "@slimio/validate-addon-name",
    "version": "1.0.0",
    "description": "SlimIO Addon name validator/sanitizer",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "test": "cross-env psp && node test/test.js",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/validate-addon-name.git"
    },
    "keywords": [
        "slimio",
        "addon",
        "name",
        "validator",
        "sanitizer"
    ],
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/validate-addon-name/issues"
    },
    "homepage": "https://github.com/SlimIO/validate-addon-name#readme",
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.3",
        "eslint": "^6.4.0",
        "husky": "^3.0.5",
        "jsdoc": "^3.6.3",
        "pkg-ok": "^2.3.1",
        "cross-env": "^6.0.0",
        "@slimio/psp": "^0.8.0",
        "japa": "^3.0.1",
        "nyc": "^14.1.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
